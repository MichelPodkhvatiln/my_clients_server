{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": "off",
    "no-console": "off",
    "prefer-destructuring": "off",
    "no-underscore-dangle": "off",
    "no-shadow": "off",
    "no-plusplus": "off"
  }
}
